[mysqld]
innodb_log_file_size=512M
innodb_buffer_pool_size=256M
innodb_flush_log_at_trx_commit=2
sync_binlog=0
server-id=333
gtid_mode=ON
enforce_gtid_consistency=true
log-bin
log-slave-updates
relay_log_info_repository=TABLE
relay_log_recovery=ON
master_info_repository=TABLE
read_only=1
report_host=mysql3
